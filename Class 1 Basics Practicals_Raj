#Print the biggest number
a=int(input("Enter a number: "))
b=int(input("Enter another number: "))
if(a>b):
    print("A is bigger than B and the value is", a)
elif(b>a):
    print("B is greater than A and the value is", b)
else:
    print("Both are same")

#print the difference
a=int(input("Enter a number: "))
b=int(input("Enter another number: "))
(a-b)

#print the difference
a=int(input("Enter a number: "))
b=int(input("Enter another number: "))
if(a>b):
    print("A is bigger than B and the difference is", a-b)
else:
    print("B is greater than A and the difference is", b-a)

